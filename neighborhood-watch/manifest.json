{
    "manifest_version": 3,
    "name": "Neighborhood Watch",
    "version": "1.0",
    "description": "Contribute and view crowdsourced labels signifying dark patterns",
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "content_scripts": [
        {
          "js": ["scripts/watch.js"],
          "matches": [
            "<all_urls>"
          ]
        }
    ]
}